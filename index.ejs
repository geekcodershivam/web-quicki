<body>

<!--profile start here-->
<h1><span>Connect</span> Contact Form</h1>
<div class="profile">
	<div class="wrap">
		<div class="content-main">
			<h2>Contact Us</h2>
			<div class="content-form">
				<form action="contactus" method="post">
					<div class="form-grid">
						<span class="Firstname label">Full Name<span class="required">*</span></span>
						<input type="text" name="Firstname" class="name" placeholder="Enter Your Full Name" required="">
						<div class="clear"></div>
					</div>
					<div class="form-grid">
						<span class="Email label">Email<span class="required">*</span></span>
						<input type="email" name="Email" class="name" placeholder="Enter Your Email" required="">
						<div class="clear"></div>
					</div>
					<div class="form-grid">
						<span class="gender label">Gender<span class="required">*</span></span>
						<select required="">
							<option value="">Select Gender</option>
							<option value="">Male</option>
							<option value="">Female</option>
						</select>
						<div class="clear"></div>
					</div>
					<div class="form-grid">
						<span class="Phone label">Phone<span class="required">*</span></span>
						<input type="text" name="Phone" class="name" placeholder="Enter Your Phone Number" required="">
						<div class="clear"></div>
					</div>
					<div class="clear"> </div>
					<div class="form-grid">
						<textarea name="Message" placeholder="Enter Your Message Here" required=""></textarea>
						<div class="clear"></div>
					</div>
					<div class="form-submit" class="maa">
						<input type="submit" value="SEND">
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

</body>
</html>